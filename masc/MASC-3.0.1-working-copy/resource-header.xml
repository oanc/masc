            <group xml:id="g.token-msd-layer">
                <g.desc>Annotations that identify all tokens in the primary data and provide
                    morphosyntactic (POS) annotations and (optionally) lemmas.</g.desc>
                <g.member value="a.penn-pos" type="type"/>
                <g.member value="a.ptbtok" type="type"/>
                <g.member value="a.fntok" type="type"/>
            </group>
            <group xml:id="g.syntactic-layer">
                <g.desc>Syntactic analysis</g.desc>
                <g.member value="a.ptb" type="type"/>
                <g.member value="a.nc" type="type"/>
                <g.member value="a.vc" type="type"/>
            </group>
            <group xml:id="g.semantic-layer">
                <g.desc>Semantic analysis</g.desc>
                <g.member value="a.ne" type="type"/>
                <g.member value="a.fn" type="type"/>
                <g.member value="a.wn" type="type"/>
                <g.member value="a.mpqa" type="type"/>
                <g.member value="a.cb" type="type"/>
                <g.member value="a.event" type="type"/>
            </group>
        </groups>
